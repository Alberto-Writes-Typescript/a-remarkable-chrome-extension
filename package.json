{
  "name": "a-remarkable-chrome-extension",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:esbuild": "node config/esbuild.config.js --production",
    "build:esbuild-dev": "node config/esbuild.config.js --development",
    "build:tailwind": "npx tailwindcss --postcss ./config/postcss.config.cjs -i ./src/index.scss -o src/App.css -c ./config/tailwindcss.config.mjs --minify",
    "build:tailwind-dev": "npx tailwindcss --postcss ./config/postcss.config.cjs -i ./src/index.scss -o src/App.css -c ./config/tailwindcss.config.mjs --watch",
    "build": "yarb build:esbuild",
    "start": "npm-run-all --parallel build:*-dev"
  },
  "type": "module",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.18",
    "dotenv": "^16.4.5",
    "esbuild": "^0.20.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.35",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-import": "^16.0.1",
    "postcss-nested": "^6.0.1",
    "postcss-preset-env": "^9.4.0",
    "postcss-scss": "^4.0.9",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "@types/react": "^18.2.62",
    "@types/react-dom": "^18.2.19",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3"
  }
}
